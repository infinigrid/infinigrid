  "files": [
    "dist"
  ],
  "dependencies": {
    "gl-matrix": "^2.3.2"
  },
  "peerDependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.14.0",
    "babelify": "^7.3.0",
    "brfs": "^1.4.3",
    "dotenv": "^2.0.0",
    "gh-pages": "^0.11.0",
    "gitbook-cli": "^2.3.0",
    "glob": "^7.1.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.0.2",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.2"
